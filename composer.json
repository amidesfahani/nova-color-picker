{
    "name": "amidesfahani/color-picker",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Amid\\ColorPicker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amid\\ColorPicker\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
